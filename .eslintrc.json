{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": 0,
        "no-alert": 0,
        "no-shadow": 0,
        "no-restricted-globals": 0,
        "radix": 0,
        "no-unused-vars": 0,
        "no-use-before-define": 0,
        "import/extensions": 0,
        "no-undef": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved" : 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "linebreak-style": 0,
        "max-classes-per-file": 0,
        "eqeqeq": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "no-new": 0,
        "import/prefer-default-export": 0,
        "class-methods-use-this": 0,
        "no-await-in-loop": 0
    }
}